{
    "name": "plan-ui",
    "version": "0.14.0",
    "main": "web-dist/plan-ui.umd.js",
    "files": [
        "web-dist",
        "web"
    ],
    "scripts": {
        "serve": "vue-cli-service serve",
        "test:unit": "vue-cli-service test:unit",
